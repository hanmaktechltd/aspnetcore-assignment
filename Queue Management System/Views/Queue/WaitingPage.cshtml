@model IEnumerable<Queue_Management_System.Models.QueueVM>

@{
    ViewData["Title"] = "WaitingPage";
}

<h1>WaitingPage</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ServicePointId)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ServicePointId)
            </td>
        </tr>
}
    </tbody>
</table>
